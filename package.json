{
    "name": "make-a-readme",
    "description": "Website for explaining what a README is",
    "version": "0.1.0",
    "scripts": {
        "dev": "yarn install && next dev",
        "build": "next build",
        "export": "next build && next export -o build",
        "format": "prettier --write \"./**/*.{json,js,jsx,ts,tsx}\"",
        "format:check": "prettier --list-different package.json \"./**/*.{json,js,jsx,ts,tsx}\""
    },
    "repository": "https://github.com/dguo/make-a-readme",
    "author": "Danny Guo",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.2.0",
        "@fortawesome/free-solid-svg-icons": "6.2.0",
        "@fortawesome/react-fontawesome": "0.2.0",
        "github-markdown-css": "4.0.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-github-corner": "2.5.0",
        "react-markdown": "5.0.3",
        "react-syntax-highlighter": "15.4.3",
        "react-textarea-autosize": "8.3.3"
    },
    "devDependencies": {
        "@types/react": "17.0.11",
        "@types/react-syntax-highlighter": "13.5.2",
        "autoprefixer": "10.2.6",
        "next": "11.1.1",
        "postcss": "8.3.5",
        "prettier": "2.3.2",
        "tailwindcss": "2.2.4",
        "typescript": "4.3.4"
    },
    "prettier": {
        "bracketSpacing": false,
        "tabWidth": 4,
        "trailingComma": "none"
    }
}
